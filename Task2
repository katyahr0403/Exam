#include <iostream>

int main()
{
    int row;
    std::cin >> row;
    for (int i = 1; i <= row; i++) {
        for (int j = 1; j <= i; j++) {
            if (j == 1 || j == i || i == row) {
                std::cout << row;
            }
            else {
                std::cout << " ";
            }
        }
        std::cout << "\n";
    }  
}
