#include <iostream>

int main()
{
    int row,col;
    std::cin >> row ;
    col = row;
    for (int i = 1; i <= row; i++)
    {
        for (int j = 1; j <= col;j++)
        {
            if (i == 1 || i == row || j == 1 || j == col)
                std::cout << row;
            else
                std::cout << " ";
        }
        std::cout << "\n";
    }
  
}
